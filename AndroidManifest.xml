<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="chal.dat255.tkp" android:versionName="2011-10-14" android:versionCode="000027">
	
	<uses-sdk android:minSdkVersion="7" />
	
	<uses-feature android:name="android.software.live_wallpaper" />
	<uses-permission android:name="android.permission.BIND_WALLPAPER"></uses-permission>
	
	<application android:icon="@drawable/icon" 
	android:label="@string/app_name" 
	android:permission="android.permission.BIND_WALLPAPER">
			<service android:label="@string/app_name"
			android:icon="@drawable/icon" android:name=".TimeKillerPetEngine">

			<intent-filter>
				<action android:name="android.service.wallpaper.WallpaperService" />
			</intent-filter>
			<meta-data android:name="android.service.wallpaper"
				android:resource="@xml/timekillerpet" />

		</service>

	</application>
</manifest>